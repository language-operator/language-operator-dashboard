{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/prebuild.js && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:organization": "jest --testPathPatterns=organization",
    "postinstall": "prisma generate || true",
    "docker:build": "docker build -t language-operator-dashboard .",
    "docker:run": "docker-compose up -d",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:stop": "docker-compose down",
    "docker:deploy": "./scripts/docker-deploy.sh",
    "db:migrate": "prisma db push",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "dev:seed": "npx tsx src/scripts/dev-seed.ts",
    "migrate:namespaces": "npx tsx src/scripts/migrate-organization-namespaces.ts",
    "initialize-tenant": "npx tsx src/scripts/initialize-tenant.ts"
  },
